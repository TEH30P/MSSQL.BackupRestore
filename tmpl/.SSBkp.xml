	<SV-SrvInst>dummy\sql</SV-SrvInst>
	<SL-DBName>dummydb</SL-DBName>
	<SL-Repo>file://a:/dummy0</SL-Repo>
	<SL-Repo>file://a:/dummy1</SL-Repo>
